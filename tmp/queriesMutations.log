# Queries
query {
 getFriend(id: "630396bfb4ce638b843a") {
  id
	firstName
  lastName
  
  language
  gender
  email
 } 
}
## List them all
query {
 getAllFriends {
   id
  	firstName
  lastName
  age
  gender
  contacts {
    firstName
    lastName
  }
  
 }
}
## Queries with list within fields
query {
 getFriend(id: "ad418be43bee4f927ffe") {
  id
	firstName
  lastName
  age
  language
  gender
  email
  contacts {
    lastName
  }
 } 
}
## Aliases on queries to run multiple queries
query {
  one: getAllFriends {
    id
    firstName
  }
  second: getOneFriend(id: "5f80edeeec472672f20f06ec"){
    id
    firstName
    lastName
  }
  aliens:getAliens{
    firstName
    planet
  }
}
## Fragment to avoid code repetition on multiple queries
query {
  one: getAllFriends {
    id
    ...nameFragment
  }
  second: getOneFriend(id: "5f80edeeec472672f20f06ec"){
    ...nameFragment
  }
}
fragment nameFragment on Friend {
  firstName
	lastName
}
## Fragment to avoid code repetition on multiple queries, sample 2
query {
  one: getAllFriends {
    id
    ...nameFragment
  }
  second: getOneFriend(id: "5f80edeeec472672f20f06ec"){
    ...nameFragment
  }
  third: getAliens {
    ...alienNameFragment
  }
}

fragment nameFragment on Friend {
  firstName
	lastName
  age
}

fragment alienNameFragment on Alien {
  id
  firstName
	lastName
}

# Mutations
mutation {
  createFriend(input: {
    firstName: "Lalo"
    lastName: "Rdgz"
    gender: "Male"
    language: "Spanish"
  }) {
    id
    firstName
  }
}
## Adding enums to restrict data types as field values on schemas
mutation {
  createFriend(input: {
    firstName: "Lalo"
    lastName: "Rdgz"
    gender: OTHER
    age:16
    language: "Spanish"
  }) {
    id
    firstName
  }
}
